{
    "name": "totov/laravel-soft-delete-morph-to-many-pivots",
    "description": "Allow morphToMany relationship pivots to be soft deleted.",
    "keywords": [
        "totov",
        "laravel",
        "laravel-soft-delete-morph-to-many-pivots"
    ],
    "homepage": "https://github.com/totov/laravel-soft-delete-morph-to-many-pivots",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephen Hamilton",
            "email": "stephen@shtc.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0||^12.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "larastan/larastan": "^1.0||^2.0||^3.0",
        "laravel/pint": "^1.0",
        "pestphp/pest-plugin-arch": "^1.0||^2.0||^3.0||^4.0",
        "pestphp/pest-plugin-laravel": "^1.0||2.0||^3.0||^4.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0||^2.0",
        "phpstan/phpstan-phpunit": "^1.0||^2.0",
        "orchestra/testbench": "^8.0||^9.0||^10.0",
        "pestphp/pest": "^1.0||^2.0||^3.0||^4.0"
    },
    "autoload": {
        "psr-4": {
            "Totov\\LaravelSoftDeleteMorphToManyPivots\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Totov\\LaravelSoftDeleteMorphToManyPivots\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-soft-delete-morph-to-many-pivots --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
